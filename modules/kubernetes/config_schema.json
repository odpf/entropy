{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "host": {
      "type": "string"
    },
    "client_key": {
      "type": "string"
    },
    "client_cert": {
      "type": "string"
    },
    "ca_cert": {
      "type": "string"
    },
    "client_timeout": {
      "type": "number",
      "default": 5000
    }
  },
  "required": [
    "host",
    "ca_cert",
    "client_cert",
    "client_key"
  ]
}