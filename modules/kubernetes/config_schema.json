{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "host": {
      "type": "string"
    },
    "insecure": {
      "type": "boolean",
      "default": false
    },
    "token": {
      "type": "string",
      "default": ""
    },
    "client_key": {
      "type": "string"
    },
    "client_certificate": {
      "type": "string"
    },
    "cluster_ca_certificate": {
      "type": "string"
    },
    "timeout": {
      "type": "number",
      "default": 100000000
    }
  },
  "required": [
    "host"
  ]
}