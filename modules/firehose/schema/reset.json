{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "to": {
      "type": "string",
      "enum": ["DATETIME", "EARLIEST", "LATEST"]
    }
  },
  "if": {
    "properties": {
      "to": {
        "const": "DATETIME"
      }
    }
  },
  "then": {
    "properties": {
      "datetime": {
        "type": "date-time"
      }
    }
  },
  "required": [
    "to"
  ]
}
